{
	"name": "itunes-app",
	"version": "0.0.1",
	"private": true,
	"author": "Jesús Díaz Rivas (https://github.com/JesusDR01)",
	"license": "MIT",
	"scripts": {
		"prepare": "husky install",
		"dev": "next dev",
		"test": "jest --watch",
		"test:ci": "jest --ci",
		"test:update": "jest --watch -u ",
		"test:coverage": "jest --coverage",
		"e2e": "start-server-and-test dev http://localhost:3000 cy:run",
		"cy:run": "cypress run --headless",
		"cy:open": "cypress open",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
		"prettier:check": "prettier --check './src/**/*.{js,ts,tsx,json,md,css}'",
		"prettier:write": "prettier --write './src/**/*.{js,ts,tsx,json,md,css}'",
		"blue-ball": "npm run lint && npm run prettier:check && pnpm test:update && pnpm build && npm pack --dry-run"
	},
	"dependencies": {
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.14.13",
		"@mui/material": "^5.0.0",
		"@mui/utils": "^5.14.13",
		"@tanstack/react-query": "^4.36.1",
		"axios": "^1.5.1",
		"clsx": "^2.0.0",
		"i18next": "^23.5.1",
		"icons-material": "@mui/icons-material",
		"jest-transform-stub": "^2.0.0",
		"next": "13.3.0",
		"next-i18next": "^14.0.3",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-i18next": "^13.2.1",
		"react-intersection-observer": "^9.5.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.1",
		"@commitlint/config-conventional": "^17.6.1",
		"@testing-library/dom": "7.21.4",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^29.5.1",
		"@types/mocha": "^10.0.3",
		"@types/node": "18.15.11",
		"@types/react": "18.0.37",
		"@types/react-dom": "18.0.11",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"autoprefixer": "10.4.14",
		"cypress": "^12.10.0",
		"daisyui": "^2.51.5",
		"eslint": "^8.38.0",
		"eslint-config-next": "^13.3.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"jsdom": "^21.1.1",
		"lint-staged": "^13.2.1",
		"postcss": "8.4.22",
		"prettier": "^2.8.7",
		"start-server-and-test": "^2.0.0",
		"tailwindcss": "3.3.1",
		"ts-jest": "^29.1.0",
		"typescript": "5.0.4"
	}
}
